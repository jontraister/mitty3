section.bios_image_content {
position:relative;
height: 169px;
width:100%;
}

div#bios_image_container {
position: absolute;
height: 100%;
width: 100%;
z-index: 2;
}

div#bios_image_container div.bios_image {
width: 100%;
height: 100%;
position: absolute;
background-position: top center;
background-size: cover;
-webkit-background-size: cover;
-moz-background-size: cover;
background-repeat: no-repeat;
top: 0;
left: 0;
display:none;
}

div#bios_image_container div.bios_image.active {
display: block;
}

div#bios_text_container {
text-align:left;
color:#ccc;
}

div#bios_text_container p {
text-align:left;
}

div#bios_text_container p.actor_title {
text-align:left;
font-size:13px;
text-transform:uppercase;
}

div#bios_text_container div.single_bio {
display:none;
}

div#bios_text_container div.single_bio.active {
display:block;
}

div#bios_text_container div.single_bio h3 {
border-bottom: 2px solid #ccc;
width: 85%;
margin: 15px 0 0 30px;
padding-bottom: 14px;
color:#ccc;
font-size:22px;
font-weight:400;
}

.carousel-control {
